{
  "name": "zod-form-generator",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "author": "Aaron Campbell <hello@aaroncampbell.design>",
  "description": "Automatically generate controlled forms from Zod schemas.",
  "homepage": "https://github.com/sleepdotexe/zod-form-generator/",
  "exports": {
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js"
    },
    "./react/components": {
      "types": "./dist/react/components/index.d.ts",
      "import": "./dist/react/components/index.js",
      "require": "./dist/react/components/index.js"
    },
    "./tailwind": {
      "style": "./styles/tailwind.css"
    },
    "./tailwind-dark": {
      "style": "./styles/tailwind-dark.css"
    }
  },
  "files": ["dist", "styles", "README.md"],
  "keywords": ["zod", "form", "generator", "typescript"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleepdotexe/zod-form-generator.git"
  },
  "bugs": {
    "url": "https://github.com/sleepdotexe/zod-form-generator/issues"
  },
  "peerDependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "libphonenumber-js": "^1.12.13",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "devDependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "libphonenumber-js": "^1.12.13",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "tsc --noEmit"
  }
}
